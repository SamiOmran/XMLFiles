<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.cisco.ifm</groupId>
    <artifactId>ifm_device_profiles</artifactId>
    <version>3.860.7-SNAPSHOT</version>
  </parent>
  <groupId>com.cisco.ifm.deviceProfile</groupId>
  <artifactId>ios-router</artifactId>
  <version>3.860.7-SNAPSHOT</version>
  <packaging>dar</packaging>
  <description>This is the generic profile for IOS routers</description>
  <properties>
    <formatVersion>2</formatVersion>
    <sdkVersion>0.9.1.201508071020</sdkVersion>
  </properties>
  <dependencies>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_memorypool</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_powerSupplyFanStatus-6k</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.deviceProfile</groupId>
      <artifactId>ios</artifactId>
      <version>${project.version}</version>
      <type>dar</type>
      <exclusions>
        <exclusion>
          <artifactId>feature_physicalresource</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature-lacp-link</artifactId>
          <groupId>com.cisco.prime.feature</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_physicalresource-6k</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_rip</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_static</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_eigrp</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_ospf</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_flashdevice</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>com.cisco.xmp.sdk</groupId>
        <artifactId>xmp-maven-dar-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
</project>
