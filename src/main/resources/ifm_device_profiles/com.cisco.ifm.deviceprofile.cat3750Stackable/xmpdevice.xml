<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.cisco.ifm</groupId>
    <artifactId>ifm_device_profiles</artifactId>
    <version>3.850.0-SNAPSHOT</version>
  </parent>
  <groupId>com.cisco.ifm.deviceprofile</groupId>
  <artifactId>cat3750Stackable</artifactId>
  <version>3.850.0-SNAPSHOT</version>
  <packaging>dar</packaging>
  <ciManagement>
    <system>Hudson</system>
  </ciManagement>
  <distributionManagement/>
  <properties>
    <formatVersion>2</formatVersion>
    <sdkVersion>0.9.1.201603291549</sdkVersion>
    <jenkins_auto.skip>true</jenkins_auto.skip>
  </properties>
  <dependencies>
    <dependency>
      <groupId>com.cisco.ifm.deviceProfile</groupId>
      <artifactId>cat3750</artifactId>
      <version>${project.version}</version>
      <type>dar</type>
      <exclusions>
        <exclusion>
          <artifactId>feature_recommendSoftware_ISR</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_verifyRecommendation_ISR</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_runningPackages_katana</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_setBootOptions_CAT3K</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_copyImageToDevice_CAT3K</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_copyImageFromDevice_CAT2K</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_upgradeAnalysis_ISR</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feature_RecommendStorage_ISR</artifactId>
          <groupId>com.cisco.ifm.feature</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_recommendSoftware_cids</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_upgradeAnalysis_cids</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_recommendStorage_cids</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_copyImageFromDevice_cids</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_copyImageToDevice_cids</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>com.cisco.xmp.sdk</groupId>
        <artifactId>xmp-maven-dar-plugin</artifactId>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
</project>
