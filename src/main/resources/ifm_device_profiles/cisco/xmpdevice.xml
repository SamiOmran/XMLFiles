      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>feature_utils</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
    <dependency>
      <groupId>com.cisco.ifm.feature</groupId>
      <artifactId>copyImageByServerFromDevice</artifactId>
      <version>${project.version}</version>
      <type>feature</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>com.cisco.xmp.sdk</groupId>
        <artifactId>xmp-maven-dar-plugin</artifactId>
